    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/eslint": "^9.6.0",
    "babel-loader": "^9.2.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "rimraf": "^6.0.1",
    "svelte": "^4.2.7",
    "svelte-check": "^3.0.0",
    "svelte-preprocess": "^5.0.0",
