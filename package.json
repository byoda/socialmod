{
  "name": "byomod",
  "version": "0.0.1",
  "type": "module",
  "dependencies": {
    "@types/chrome": "^0.0.273",
    "chrome-types": "^0.1.301",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "ts-loader": "^9.5.1",
    "url": "^0.11.4",
    "webpack": "^5.95.0"
  },
  "scripts": {
    "build_ext": "webpack --mode development",
    "dev": "vite --config tsconfig-vite.json dev",
    "build": "vite --config tsconfig-vite.json build",
    "preview": "vite --config tsconfig-vite.json preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-check",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
